        <MountPoint Side="Back" StartX="0" StartY="0" EndX="1" EndY="1" Default="true" />		
        <MountPoint Side="Top" StartX="0" StartY="0" EndX="0.1" EndY="0.1" />
        <MountPoint Side="Bottom" StartX="0" StartY="0" EndX="0.1" EndY="0.1" />
        <MountPoint Side="Left" StartX="0" StartY="0" EndX="0.1" EndY="0.1" />
        <MountPoint Side="Right" StartX="0" StartY="0" EndX="0.1" EndY="0.1" />
      </MountPoints>
      <BuildProgressModels>
        <Model BuildPercentUpperBound="0.66" File="Models\Cubes\Large\RCSLBS_nukeguard_Constr1.mwm" />
        <Model BuildPercentUpperBound="1.00" File="Models\Cubes\Large\RCSLBS_nukeguard_Constr2.mwm" />
      </BuildProgressModels>
      <BlockPairName>RCSnuke</BlockPairName>
      <ThrusterType>Hydrogen</ThrusterType>
      <Center x="0" y="0" z="0" />
      <MirroringZ>Y</MirroringZ>
	  <MirroringY>Z</MirroringY>
      <EdgeType>Light</EdgeType>
      <BuildTimeSeconds>25</BuildTimeSeconds>
      <ResourceSinkGroup>Thrust</ResourceSinkGroup>
      <ForceMagnitude>112500</ForceMagnitude>
      <SlowdownFactor>1</SlowdownFactor>
      <FuelConverter>
        <FuelId>
          <TypeId>GasProperties</TypeId>
          <SubtypeId>Hydrogen</SubtypeId>
        </FuelId>
        <Efficiency>24</Efficiency>
      </FuelConverter>
      <MaxPowerConsumption>0.7</MaxPowerConsumption>
      <MinPowerConsumption>0.000000001</MinPowerConsumption>
      <FlameDamageLengthScale>0.0</FlameDamageLengthScale>
      <FlameLengthScale>0.85</FlameLengthScale>
      <FlameIdleColor>
        <X>0.2</X>
        <Y>0.2</Y>
        <Z>0.5</Z>
        <W>0.75</W>
      </FlameIdleColor>
      <FlameFullColor>
        <X>1</X>
        <Y>0.75</Y>
        <Z>0.2</Z>
        <W>0.75</W>
      </FlameFullColor>
      <FlamePointMaterial>EngineThrustMiddle</FlamePointMaterial>
      <FlameLengthMaterial>EngineThrustMiddle</FlameLengthMaterial>
      <FlameGlareMaterial>GlareLsThrustSmall</FlameGlareMaterial>
      <FlameVisibilityDistance>200</FlameVisibilityDistance>
      <FlameGlareSize>0.585</FlameGlareSize>
      <FlameGlareQuerySize>2</FlameGlareQuerySize>
      <SilenceableByShipSoundSystem>true</SilenceableByShipSoundSystem>
      <PrimarySound>SmShipSmJetHydrogen</PrimarySound>
      <DamageEffectId>213</DamageEffectId>
      <DamagedSound>ParticleWeapExpl</DamagedSound>
      <Public>true</Public>
    </Definition>   

