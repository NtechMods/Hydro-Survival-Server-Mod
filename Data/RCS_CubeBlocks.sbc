      <ModelOffset x="0" y="0" z="0" />
      <Model>Models\Cubes\Large\RCSLBS_nukeguard.mwm</Model>
      <Components>
        <Component Subtype="SteelPlate" Count="10" />
        <Component Subtype="Construction" Count="10" />
        <Component Subtype="MetalGrid" Count="8" />
        <Component Subtype="LargeTube" Count="1" />
        <Component Subtype="SmallTube" Count="16" />
        <Component Subtype="Construction" Count="20" />
        <Component Subtype="SteelPlate" Count="8" />
      </Components>
      <CriticalComponent Subtype="LargeTube" Index="0" />
